{
  "name": "my-app",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "electron-updater": "^4.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "build": {
    "appId": "com.leon.my-app",
    "copyright": "LEON",
    "productName": "my-app",
    "publish": [ 
      { 
        "provider": "generic",
        "url": "http://192.168.0.226/electron/"
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "electron": "electron .",
    "dist": "electron-builder --mac",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "doc": "vuepress dev docs"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^6.0.4",
    "electron-builder": "^21.2.0",
    "vuepress": "1.0.3"
  }
}
